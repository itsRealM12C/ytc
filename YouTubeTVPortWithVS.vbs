Set objShell = CreateObject("WScript.Shell")

' 1. Detection: Get Screen Resolution to Center Window
Set objWMIService = GetObject("winmgmts:\\.\root\cimv2")
Set colItems = objWMIService.ExecQuery("Select * from Win32_VideoController",,48)
For Each objItem in colItems
    intScreenWidth = objItem.CurrentHorizontalResolution
    intScreenHeight = objItem.CurrentVerticalResolution
Next

' 2. Prepare base64 HTML in chunks (avoid long literal limits)
strB64 = ""
strB64 = strB64 & ""
