strB64 = strB64 & "MDAwKTsKPC9zY3JpcHQ+CgogIDwvYm9keT4KPC9odG1sPgo="

' 3. Decode base64 and write to a temp HTML file
tempPath = objShell.ExpandEnvironmentStrings("%TEMP%") & "\\YouTube.html"
Set xml = CreateObject("MSXML2.DOMDocument.3.0")
Set elem = xml.createElement("b64")
elem.dataType = "bin.base64"
elem.text = strB64
decoded = elem.nodeTypedValue
Set stream = CreateObject("ADODB.Stream")
stream.Type = 1
stream.Open
stream.Write decoded
stream.SaveToFile tempPath, 2
stream.Close

' 4. Configuration and launch using file:// URL
strURL = "file:///" & Replace(tempPath, "\\", "/")
strEdgePath = """C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe"""
strUA = """Mozilla/5.0 Cobalt/25"""
intWidth = 1280
intHeight = 720
intPosX = (intScreenWidth - intWidth) / 2
intPosY = (intScreenHeight - intHeight) / 2
strArgs = " --app=" & strURL & _
          " --user-agent=" & strUA & _
          " --window-size=" & intWidth & "," & intHeight & _
          " --window-position=" & intPosX & "," & intPosY & _
          " --user-data-dir=%TEMP%\\YouTubeTV"

objShell.Run strEdgePath & " " & strArgs, 1, False